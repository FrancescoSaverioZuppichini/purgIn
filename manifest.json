{
  "manifest_version": 3,
  "name": "PurgeIn",
  "description": "Purge your LikenIn feed from stuff you don't want to see.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://www.linkedin.com/*", "https://www.linkedin.com/feed/*"]
    }
  ],
  "permissions": [
    "storage"
  ]
}
